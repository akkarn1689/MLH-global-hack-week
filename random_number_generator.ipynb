    "from datetime import datetime\n",
    "import math\n",
    "\n",
    "num = datetime.now().microsecond\n",
    "\n",
    "num /=1000000\n",
    "\n",
    "n = int(input(\"Enter the number upto which you want to generate random numbers: \"))\n",
    "\n",
    "randomNum = math.ceil(n*num + 1);\n",
    "\n",
    "# print(num)\n",
    "\n",
    "print(randomNum)"
   ]
